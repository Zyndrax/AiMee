# AiMee Environment Variables Example
# Copy this file to .env and fill in the values

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-supabase-api-key
SUPABASE_TABLE_NAME=embeddings

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORGANIZATION=your-openai-organization-id  # Optional
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=1536

# Anthropic Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_EMBEDDING_MODEL=claude-3-haiku-20240307

# Vector Database Configuration
VECTOR_DB_PROVIDER=supabase  # Options: supabase, pinecone, etc.

# Embedding Configuration
EMBEDDING_PROVIDER=openai  # Options: openai, anthropic, etc.
EMBEDDING_MODEL=text-embedding-3-small  # Model name for the selected provider
EMBEDDING_BATCH_SIZE=100  # Maximum batch size for embedding requests

# Debug mode
DEBUG=false 